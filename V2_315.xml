      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d12_s1">
      <text>sporangia glabrous.</text>
      <biological_entity id="from_key_V2_451_o11048" name="sporangium" name_original="sporangia" src="d12_s1" type="structure">
        <character is_modifier="false" name="pubescence" src="d12_s1" value="glabrous" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d6_s0">
      <text>Stellate or forked hairs absent.</text>
      <biological_entity id="from_key_V2_451_o11018" name="whole_organism" name_original="" src="d6_s0" type="structure">
