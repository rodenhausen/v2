        <character is_modifier="false" name="count" src="d6_s0" value="absent" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d2_s0">
      <text>Pinnae shallowly to deeply lobed;</text>
      <biological_entity id="from_key_V2_451_o11001" name="pinna" name_original="pinnae" src="d2_s0" type="structure">
        <character is_modifier="false" modifier="shallowly to deeply" name="shape" src="d2_s0" value="lobed" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d2_s1">
