    <statement id="from_key_V2_451_d36_s5">
      <text>12.</text>
    </statement>
    <statement id="from_key_V2_451_d36_s6">
      <text>Thelypteris kunthii</text>
      <biological_entity id="from_key_V2_451_o11182" name="vein" name_original="veins" src="d36_s4" type="structure" />
    </statement>
    <statement id="from_key_V2_451_d33_s0">
      <text>Costules and veins adaxially with at least a few rather stout hairs mostly longer than 0.3 mm;</text>
      <biological_entity id="from_key_V2_451_o11146" name="costule" name_original="costules" src="d33_s0" type="structure" />
      <biological_entity id="from_key_V2_451_o11147" name="vein" name_original="veins" src="d33_s0" type="structure" />
      <biological_entity id="from_key_V2_451_o11148" name="hair" name_original="hairs" src="d33_s0" type="structure">
        <character is_modifier="true" modifier="at-least" name="count" src="d33_s0" value="few" />
        <character is_modifier="true" modifier="rather" name="fragility_or_size" src="d33_s0" value="stout" />
        <character modifier="mostly longer than" name="some_measurement" src="d33_s0" unit="mm" value="0.3" />
      </biological_entity>
      <relation from="from_key_V2_451_o11146" id="from_key_V2_451_r2552" name="with" negation="false" src="d33_s0" to="from_key_V2_451_o11148" />
      <relation from="from_key_V2_451_o11147" id="from_key_V2_451_r2553" name="with" negation="false" src="d33_s0" to="from_key_V2_451_o11148" />
    </statement>
    <statement id="from_key_V2_451_d33_s1">
      <text>blades adaxially often rather glandular;</text>
      <biological_entity id="from_key_V2_451_o11149" name="blade" name_original="blades" src="d33_s1" type="structure">
        <character is_modifier="false" modifier="adaxially often rather" name="architecture_or_function_or_pubescence" src="d33_s1" value="glandular" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d33_s2">
      <text>scales absent on abaxial surface of rachises and costae of mature leaves.</text>
      <biological_entity id="from_key_V2_451_o11150" name="scale" name_original="scales" src="d33_s2" type="structure">
        <character constraint="on abaxial surface" constraintid="o11151" is_modifier="false" name="count" src="d33_s2" value="absent" />
      </biological_entity>
      <biological_entity constraint="abaxial" id="from_key_V2_451_o11151" name="surface" name_original="surface" src="d33_s2" type="structure" />
      <biological_entity id="from_key_V2_451_o11152" name="rachis" name_original="rachises" src="d33_s2" type="structure" />
      <biological_entity id="from_key_V2_451_o11153" name="costa" name_original="costae" src="d33_s2" type="structure" />
      <biological_entity id="from_key_V2_451_o11154" name="leaf" name_original="leaves" src="d33_s2" type="structure">
        <character is_modifier="true" name="life_cycle" src="d33_s2" value="mature" />
      </biological_entity>
      <relation from="from_key_V2_451_o11151" id="from_key_V2_451_r2554" name="part_of" negation="false" src="d33_s2" to="from_key_V2_451_o11152" />
      <relation from="from_key_V2_451_o11151" id="from_key_V2_451_r2555" name="part_of" negation="false" src="d33_s2" to="from_key_V2_451_o11153" />
      <relation from="from_key_V2_451_o11151" id="from_key_V2_451_r2556" name="part_of" negation="false" src="d33_s2" to="from_key_V2_451_o11154" />
    </statement>
    <statement id="from_key_V2_451_d32_s0">
      <text>Stems long to short-creeping;</text>
      <biological_entity id="from_key_V2_451_o11142" name="stem" name_original="stems" src="d32_s0" type="structure">
        <character is_modifier="false" name="length_or_size" src="d32_s0" value="long" />
        <character is_modifier="false" name="growth_form_or_orientation" src="d32_s0" value="short-creeping" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d32_s1">
      <text>scales at base of petioles lanceolate, usually hairy.</text>
      <biological_entity id="from_key_V2_451_o11143" name="scale" name_original="scales" src="d32_s1" type="structure">
        <character is_modifier="false" modifier="usually" name="pubescence" notes="alterIDs:o11144" src="d32_s1" value="hairy" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11144" name="base" name_original="base" src="d32_s1" type="structure">
        <character is_modifier="false" name="shape" src="d32_s1" value="lanceolate" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11145" name="petiole" name_original="petioles" src="d32_s1" type="structure" />
      <relation from="from_key_V2_451_o11143" id="from_key_V2_451_r2550" name="at" negation="false" src="d32_s1" to="from_key_V2_451_o11144" />
