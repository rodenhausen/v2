        <character is_modifier="true" name="arrangement" src="d14_s0" value="connivent" />
        <character is_modifier="true" name="fusion" src="d14_s0" value="united" />
      </biological_entity>
      <relation from="from_key_V2_451_o11054" id="from_key_V2_451_r2526" name="with" negation="false" src="d14_s0" to="from_key_V2_451_o11055" />
      <relation from="from_key_V2_451_o11055" id="from_key_V2_451_r2527" name="extending to" negation="false" src="d14_s0" to="from_key_V2_451_o11056" />
    </statement>
    <statement id="from_key_V2_451_d14_s1">
      <text>proximal pinnae usually not reduced (subg. Cyclosorus), or if reduced, then veins united below sinus.</text>
      <biological_entity constraint="proximal" id="from_key_V2_451_o11057" name="pinna" name_original="pinnae" src="d14_s1" type="structure">
        <character is_modifier="false" modifier="usually not" name="size" src="d14_s1" value="reduced" />
        <character is_modifier="false" name="size" src="d14_s1" value="reduced" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11058" name="vein" name_original="veins" src="d14_s1" type="structure" />
      <biological_entity id="from_key_V2_451_o11059" name="sinus" name_original="sinus" src="d14_s1" type="structure" />
      <relation from="from_key_V2_451_o11058" id="from_key_V2_451_r2528" name="united" negation="false" src="d14_s1" to="from_key_V2_451_o11059" />
    </statement>
    <statement id="from_key_V2_451_d12_s0">
      <text>Sori round or slightly oblong;</text>
      <biological_entity id="from_key_V2_451_o11047" name="sorus" name_original="sori" src="d12_s0" type="structure">
        <character is_modifier="false" name="shape" src="d12_s0" value="round" />
