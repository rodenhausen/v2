    <statement id="from_key_V2_440_d17_s2">
      <text>stems usually short-creeping, leaves clustered (separated to 10 mm in C. clevelandii).</text>
      <biological_entity id="from_key_V2_440_o10510" name="stem" name_original="stems" src="d17_s2" type="structure">
        <character is_modifier="false" modifier="usually" name="growth_form_or_orientation" src="d17_s2" value="short-creeping" />
      </biological_entity>
      <biological_entity id="from_key_V2_440_o10511" name="leaf" name_original="leaves" src="d17_s2" type="structure">
        <character is_modifier="false" name="arrangement_or_growth_form" src="d17_s2" value="clustered" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_440_d12_s0">
      <text>Costal scale margins ciliate, especially near base (inconspicuously so in C. covillei).</text>
      <biological_entity constraint="costal scale" id="from_key_V2_440_o10484" name="margin" name_original="margins" src="d12_s0" type="structure">
        <character is_modifier="false" name="architecture_or_pubescence_or_shape" src="d12_s0" value="ciliate" />
