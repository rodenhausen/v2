    </statement>
    <statement id="from_key_V2_440_d8_s0">
      <text>Costal scales lanceolate to ovate, conspicuous, the largest 0.4-1.5 mm wide.</text>
      <biological_entity constraint="costal" id="from_key_V2_440_o10473" name="scale" name_original="scales" src="d8_s0" type="structure">
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d8_s0" value="lanceolate" />
