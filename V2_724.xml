        <character is_modifier="false" name="fragility" src="d6_s0" value="stiff" />
      </biological_entity>
      <biological_entity id="from_key_V2_440_o10469" name="touch" name_original="touch" src="d6_s0" type="structure" />
      <biological_entity id="from_key_V2_440_o10470" name="hair" name_original="hairs" src="d6_s0" type="structure">
        <character is_modifier="true" name="relief" src="d6_s0" value="pustulose" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_440_d6_s1">
      <text>fertile ultimate segments round to somewhat elliptic, the largest less than 3 mm.</text>
      <biological_entity constraint="ultimate" id="from_key_V2_440_o10471" name="segment" name_original="segments" src="d6_s1" type="structure">
        <character is_modifier="true" name="reproduction" src="d6_s1" value="fertile" />
