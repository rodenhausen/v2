        <character is_modifier="false" modifier="slightly; adaxially" name="shape" src="d4_s0" value="flattened" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_440_d4_s1">
      <text>ultimate segments round, elliptic, or oblong;</text>
      <biological_entity constraint="ultimate" id="from_key_V2_440_o10462" name="segment" name_original="segments" src="d4_s1" type="structure">
        <character is_modifier="false" name="shape" src="d4_s1" value="round" />
