        <character is_modifier="false" modifier="somewhat" name="arrangement_or_shape" src="d6_s1" value="elliptic" />
        <character is_modifier="false" name="size" src="d6_s1" value="largest" />
        <character char_type="range_value" from="0" from_unit="mm" name="some_measurement" src="d6_s1" to="3" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_440_d4_s0">
      <text>Rachises and petioles rounded or slightly flattened adaxially;</text>
      <biological_entity id="from_key_V2_440_o10460" name="rachis" name_original="rachises" src="d4_s0" type="structure">
        <character is_modifier="false" name="shape" src="d4_s0" value="rounded" />
