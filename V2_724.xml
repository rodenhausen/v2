        <character is_modifier="false" name="size" src="d8_s0" value="largest" />
        <character char_type="range_value" from="0.4" from_unit="mm" name="width" src="d8_s0" to="1.5" to_unit="mm" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_440_d6_s0">
      <text>Ultimate segments smooth to touch, lacking stiff, pustulose hairs;</text>
      <biological_entity constraint="ultimate" id="from_key_V2_440_o10468" name="segment" name_original="segments" src="d6_s0" type="structure">
        <character constraint="to touch" constraintid="o10469" is_modifier="false" name="architecture_or_pubescence_or_relief" src="d6_s0" value="smooth" />
