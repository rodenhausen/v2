        <character char_type="range_value" from="1" from_unit="mm" name="width" src="d0_s2" to="3" to_unit="mm" />
        <character is_modifier="false" name="shape" notes="" src="d0_s2" value="ovate" />
        <character is_modifier="false" name="pubescence" src="d0_s2" value="glabrous" />
      </biological_entity>
      <biological_entity id="o3515" name="base" name_original="base" src="d0_s2" type="structure" />
