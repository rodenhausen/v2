        <character is_modifier="false" name="architecture" src="d6_s4" value="winged" />
        <character is_modifier="false" name="architecture" src="d6_s4" value="wingless" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d6_s5">
      <text>trees of various elevations.</text>
      <biological_entity id="from_key_V2_300_o6343" name="tree" name_original="trees" src="d6_s5" type="structure" />
    </statement>
    <statement id="from_key_V2_300_d3_s0">
      <text>Leaves mostly 5 per fascicle, 3-sided, straight or curved;</text>
      <biological_entity id="from_key_V2_300_o6317" name="fascicle" name_original="fascicle" src="d3_s0" type="structure" />
    </statement>
    <statement id="from_key_V2_300_d3_s1">
