      <biological_entity id="from_key_V2_300_o6404" name="opening" name_original="opening" src="d18_s0" type="structure" />
      <biological_entity id="from_key_V2_300_o6405" name="scale" name_original="scales" src="d18_s0" type="structure">
        <character is_modifier="false" modifier="firmly" name="fixation" src="d18_s0" value="attached" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d12_s0">
      <text>Seed-wing lacking or shorter than seed body;</text>
      <biological_entity id="from_key_V2_300_o6373" name="seed-wing" name_original="seed-wing" src="d12_s0" type="structure">
        <character is_modifier="false" name="count" src="d12_s0" value="lacking" />
        <character constraint="than seed body" constraintid="o6374" is_modifier="false" name="height_or_length_or_size" src="d12_s0" value="shorter" />
      </biological_entity>
      <biological_entity constraint="seed" id="from_key_V2_300_o6374" name="body" name_original="body" src="d12_s0" type="structure" />
    </statement>
    <statement id="from_key_V2_300_d12_s1">
      <text>apophysis mostly distinctly thicker than subtending portion of seed-cone scale;</text>
      <biological_entity id="from_key_V2_300_o6375" name="apophysis" name_original="apophysis" src="d12_s1" type="structure">
        <character constraint="than subtending portion" constraintid="o6376" is_modifier="false" name="width" src="d12_s1" value="mostly distinctly thicker" />
      </biological_entity>
      <biological_entity constraint="subtending" id="from_key_V2_300_o6376" name="portion" name_original="portion" src="d12_s1" type="structure" />
      <biological_entity constraint="seed-cone" id="from_key_V2_300_o6377" name="scale" name_original="scale" src="d12_s1" type="structure" />
      <relation from="from_key_V2_300_o6376" id="from_key_V2_300_r1470" name="part_of" negation="false" src="d12_s1" to="from_key_V2_300_o6377" />
    </statement>
    <statement id="from_key_V2_300_d12_s2">
      <text>leaves straight or curved, persisting 5 years or more (except P. strobiformis);</text>
    </statement>
    <statement id="from_key_V2_300_d12_s3">
      <text>medium-sized to low trees mostly of high elevations.</text>
      <biological_entity id="from_key_V2_300_o6378" name="leaf" name_original="leaves" src="d12_s2" type="structure">
        <character is_modifier="false" name="course" src="d12_s2" value="straight" />
        <character is_modifier="false" name="course" src="d12_s2" value="curved" />
        <character is_modifier="false" name="duration" src="d12_s2" value="persisting" />
        <character name="count" src="d12_s2" value="5" />
        <character is_modifier="false" name="size" src="d12_s3" value="medium-sized" />
      </biological_entity>
      <biological_entity id="from_key_V2_300_o6379" name="tree" name_original="trees" src="d12_s3" type="structure">
        <character is_modifier="true" name="position" src="d12_s3" value="low" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d6_s0">
      <text>Fascicles persistent 8 or fewer years, not forming a long brush;</text>
