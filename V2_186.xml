        <character is_modifier="false" name="architecture" src="d3_s2" value="winged" />
        <character is_modifier="false" name="architecture" src="d3_s2" value="wingless" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d1_s0">
