        <character is_modifier="false" name="course" src="d6_s1" value="straight" />
        <character is_modifier="false" name="course" src="d6_s1" value="curved" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d6_s2">
      <text>apophyses variously thickened, umbos terminal;</text>
      <biological_entity id="from_key_V2_300_o6338" name="apophysis" name_original="apophyses" src="d6_s2" type="structure">
        <character is_modifier="false" modifier="variously" name="size_or_width" src="d6_s2" value="thickened" />
      </biological_entity>
      <biological_entity id="from_key_V2_300_o6339" name="umbo" name_original="umbos" src="d6_s2" type="structure">
        <character is_modifier="false" name="position_or_structure_subtype" src="d6_s2" value="terminal" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_300_d6_s3">
      <text>mature seed-cones variously shaped, pale-brown to gray or gray-brown (purplish in P. albicaulis), without prickles;</text>
