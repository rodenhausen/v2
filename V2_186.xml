        <character is_modifier="true" name="life_cycle" src="d18_s0" value="mature" />
        <character is_modifier="false" name="shape" src="d18_s0" value="lanceovoid" />
        <character constraint="before opening" constraintid="o6404" is_modifier="false" name="shape" src="d18_s0" value="lance-cylindric" />
