        <character is_modifier="false" name="coloration" notes="[duplicate value]" src="d6_s3" value="pale-brown" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" src="d6_s3" value="gray" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" src="d6_s3" value="gray-brown" />
