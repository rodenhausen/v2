        <character is_modifier="false" name="arrangement" src="d6_s0" value="fascicles" />
        <character is_modifier="false" name="duration" src="d6_s0" value="persistent" />
        <character name="count" src="d6_s0" unit="or feweryears" value="8" />
      </biological_entity>
      <biological_entity id="from_key_V2_300_o6336" name="brush" name_original="brush" src="d6_s0" type="structure">
        <character is_modifier="true" name="length_or_size" src="d6_s0" value="long" />
      </biological_entity>
      <relation from="from_key_V2_300_o6335" id="from_key_V2_300_r1459" name="forming a" negation="true" src="d6_s0" to="from_key_V2_300_o6336" />
    </statement>
    <statement id="from_key_V2_300_d6_s1">
      <text>leaves straight or curved, of various lengths;</text>
