        <character is_modifier="false" modifier="occasionally" name="condition" src="d0_s0" value="stranded" />
      </biological_entity>
      <biological_entity id="o10882" name="mud" name_original="mud" src="d0_s0" type="structure" />
      <relation from="o10881" id="r2471" name="subsisting on" negation="false" src="d0_s0" to="o10882" />
    </statement>
    <statement id="d0_s1">
      <text>plants heterosporous (producing 2 kinds of spores), leptosporangiate, proliferous by axillary fragmentation.</text>
