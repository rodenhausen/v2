        <character is_modifier="false" name="growth_form_or_orientation" src="d4_s0" value="prostrate" />
        <character is_modifier="false" name="orientation" src="d4_s0" value="creeping" />
        <character is_modifier="false" name="orientation" src="d4_s0" value="erect" />
        <character is_modifier="false" modifier="never" name="growth_form" src="d4_s0" value="rosette-forming" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_812_d4_s1">
      <text>leaves delicate, papery;</text>
    </statement>
    <statement id="from_key_V2_812_d4_s2">
      <text>growing in mild humid areas, very often in shady places.</text>
      <biological_entity id="from_key_V2_812_o23741" name="leaf" name_original="leaves" src="d4_s1" type="structure">
        <character is_modifier="false" name="fragility" src="d4_s1" value="delicate" />
