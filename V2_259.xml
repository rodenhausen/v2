      <relation from="from_key_V2_812_o23729" id="from_key_V2_812_r5541" name="on" negation="false" src="d2_s1" to="from_key_V2_812_o23730" />
      <relation from="from_key_V2_812_o23730" id="from_key_V2_812_r5542" name="part_of" negation="false" src="d2_s1" to="from_key_V2_812_o23731" />
      <relation from="from_key_V2_812_o23729" id="from_key_V2_812_r5543" modifier="seldom" name="on" negation="false" src="d2_s1" to="from_key_V2_812_o23732" />
    </statement>
    <statement id="from_key_V2_812_d2_s2">
      <text>strobili with many megasporangia, megasporangia basal or throughout length of strobilus;</text>
      <biological_entity id="from_key_V2_812_o23733" name="strobilus" name_original="strobili" src="d2_s2" type="structure" />
      <biological_entity id="from_key_V2_812_o23734" name="strobilus" name_original="strobilus" src="d2_s2" type="structure">
        <character is_modifier="true" name="character" src="d2_s2" value="length" />
      </biological_entity>
      <relation from="from_key_V2_812_o23733" id="from_key_V2_812_r5544" name="with many megasporangia , megasporangia basal or throughout" negation="false" src="d2_s2" to="from_key_V2_812_o23734" />
    </statement>
    <statement id="from_key_V2_812_d2_s3">
      <text>megasporophylls and microsporophylls similar (except in S. apoda complex).</text>
