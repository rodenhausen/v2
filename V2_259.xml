        <character is_modifier="false" name="growth_form_or_orientation" src="d12_s1" value="short-creeping" />
        <character is_modifier="false" name="growth_form_or_orientation" src="d12_s1" value="long-creeping" />
      </biological_entity>
      <biological_entity constraint="lateral" id="from_key_V2_812_o23780" name="branch" name_original="branches" src="d12_s1" type="structure">
        <character is_modifier="false" name="shape" src="d12_s1" value="1-2-forked" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_812_d8_s0">
      <text>Plants with rather short-branched, prostrate or creeping stems.</text>
      <biological_entity id="from_key_V2_812_o23759" name="plant" name_original="plants" src="d8_s0" type="structure" />
      <biological_entity id="from_key_V2_812_o23760" name="stem" name_original="stems" src="d8_s0" type="structure">
        <character is_modifier="true" modifier="rather" name="architecture" src="d8_s0" value="short-branched" />
        <character is_modifier="true" name="growth_form_or_orientation" src="d8_s0" value="prostrate" />
        <character is_modifier="true" name="growth_form_or_orientation" src="d8_s0" value="creeping" />
      </biological_entity>
      <relation from="from_key_V2_812_o23759" id="from_key_V2_812_r5550" name="with" negation="false" src="d8_s0" to="from_key_V2_812_o23760" />
    </statement>
    <statement id="from_key_V2_812_d4_s0">
      <text>Stems prostrate, creeping or erect, never rosette-forming;</text>
      <biological_entity id="from_key_V2_812_o23740" name="stem" name_original="stems" src="d4_s0" type="structure">
        <character is_modifier="false" name="growth_form_or_orientation" src="d4_s0" value="prostrate" />
        <character is_modifier="false" name="orientation" src="d4_s0" value="creeping" />
        <character is_modifier="false" name="orientation" src="d4_s0" value="erect" />
        <character is_modifier="false" modifier="never" name="growth_form" src="d4_s0" value="rosette-forming" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_812_d4_s1">
      <text>leaves delicate, papery;</text>
    </statement>
    <statement id="from_key_V2_812_d4_s2">
      <text>growing in mild humid areas, very often in shady places.</text>
      <biological_entity id="from_key_V2_812_o23741" name="leaf" name_original="leaves" src="d4_s1" type="structure">
        <character is_modifier="false" name="fragility" src="d4_s1" value="delicate" />
        <character is_modifier="false" name="texture" src="d4_s1" value="papery" />
      </biological_entity>
      <biological_entity id="from_key_V2_812_o23742" name="area" name_original="areas" src="d4_s2" type="structure">
        <character is_modifier="true" name="taste" src="d4_s2" value="mild" />
      </biological_entity>
      <biological_entity id="from_key_V2_812_o23743" name="place" name_original="places" src="d4_s2" type="structure">
        <character is_modifier="true" name="habitat" src="d4_s2" value="shady" />
      </biological_entity>
      <relation from="from_key_V2_812_o23741" id="from_key_V2_812_r5546" name="growing in" negation="false" src="d4_s2" to="from_key_V2_812_o23742" />
    </statement>
    <statement id="from_key_V2_812_d2_s0">
      <text>Stems not articulate;</text>
      <biological_entity id="from_key_V2_812_o23728" name="stem" name_original="stems" src="d2_s0" type="structure">
        <character is_modifier="false" modifier="not" name="architecture" src="d2_s0" value="articulate" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_812_d2_s1">
      <text>rhizophores on underside of stems, axillary, or seldom on upperside (S. lepidophylla);</text>
      <biological_entity id="from_key_V2_812_o23729" name="rhizophore" name_original="rhizophores" src="d2_s1" type="structure">
        <character is_modifier="false" name="position" notes="alterIDs:o23730" src="d2_s1" value="axillary" />
      </biological_entity>
      <biological_entity id="from_key_V2_812_o23730" name="underside" name_original="underside" src="d2_s1" type="structure" />
      <biological_entity id="from_key_V2_812_o23731" name="stem" name_original="stems" src="d2_s1" type="structure" />
