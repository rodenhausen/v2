      </biological_entity>
    </statement>
    <statement id="from_key_V2_812_d16_s2">
      <text>plants not minute, forming mats larger than 6 cm diam.</text>
      <biological_entity id="from_key_V2_812_o23799" name="plant" name_original="plants" src="d16_s2" type="structure">
