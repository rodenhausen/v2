    </statement>
    <statement id="from_key_V2_812_d8_s0">
      <text>Plants with rather short-branched, prostrate or creeping stems.</text>
      <biological_entity id="from_key_V2_812_o23759" name="plant" name_original="plants" src="d8_s0" type="structure" />
