  <description type="morphology">
    <statement id="d0_s0">
      <text>Plants tufted, grasslike, heterosporous (megaspores and microspores not alike), perennial;</text>
    </statement>
    <statement id="d0_s1">
      <text>evergreen aquatics to ephemeral terrestrials.</text>
      <biological_entity id="o23906" name="plant" name_original="plants" src="d0_s0" type="structure">
        <character is_modifier="false" name="arrangement_or_pubescence" src="d0_s0" value="tufted" />
        <character is_modifier="false" name="growth_form" src="d0_s0" value="grasslike" />
        <character is_modifier="false" name="reproduction" src="d0_s0" value="heterosporous" />
        <character is_modifier="false" name="duration" src="d0_s0" value="perennial" />
        <character is_modifier="false" name="duration" src="d0_s1" value="evergreen" />
      </biological_entity>
      <biological_entity id="o23907" name="terrestrial" name_original="terrestrials" src="d0_s1" type="structure">
        <character is_modifier="false" name="growth_form_or_habitat" src="d0_s1" value="aquatics" />
        <character is_modifier="true" name="duration" src="d0_s1" value="ephemeral" />
