      <biological_entity id="o23909" name="root" name_original="roots" src="d0_s3" type="structure">
        <character constraint="along central groove" constraintid="o23910" is_modifier="false" name="orientation" src="d0_s3" value="arising" />
        <character is_modifier="false" name="architecture" src="d0_s3" value="simple" />
        <character is_modifier="false" modifier="dichotomously" name="architecture" src="d0_s3" value="branched" />
      </biological_entity>
      <biological_entity constraint="central" id="o23910" name="groove" name_original="groove" src="d0_s3" type="structure">
        <character is_modifier="false" name="arrangement" src="d0_s3" value="separating" />
