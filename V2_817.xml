        <character is_modifier="true" modifier="frequently" name="quantity" src="d0_s4" value="several" />
        <character is_modifier="true" name="texture" src="d0_s4" value="fibrous" />
      </biological_entity>
      <relation from="o23914" id="r5591" name="containing" negation="false" src="d0_s4" to="o23917" />
