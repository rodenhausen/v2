      <biological_entity id="from_key_V2_301_o6805" name="shrub" name_original="shrubs" src="d16_s2" type="structure">
        <character is_modifier="true" modifier="usually" name="architecture_or_growth_form" src="d16_s2" value="multistemmed" />
        <character char_type="range_value" from="0" from_unit="m" name="some_measurement" src="d16_s2" to="12" to_unit="m" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6806" name="tree" name_original="trees" src="d16_s2" type="structure">
