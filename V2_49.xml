    <statement id="from_key_V2_301_d20_s1">
      <text>branchlets ca. 2/3 as wide as length of scalelike leaves;</text>
      <biological_entity id="from_key_V2_301_o6818" name="branchlet" name_original="branchlets" src="d20_s1" type="structure">
        <character constraint="as-wide-as " constraintid="o6819" name="count" src="d20_s1" value="2/3" />
