        <character is_modifier="true" name="shape" src="d20_s2" value="scalelike" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6821" name="apex" name_original="apex" src="d20_s2" type="structure">
        <character is_modifier="false" name="orientation" src="d20_s2" value="spreading" />
      </biological_entity>
      <biological_entity constraint="abaxial" id="from_key_V2_301_o6822" name="surface" name_original="surface" src="d20_s2" type="structure">
        <character is_modifier="false" name="prominence" src="d20_s2" value="raised" />
      </biological_entity>
      <relation from="from_key_V2_301_o6820" id="from_key_V2_301_r1587" name="with" negation="false" src="d20_s2" to="from_key_V2_301_o6821" />
    </statement>
    <statement id="from_key_V2_301_d20_s3">
      <text>branchlets generally 3-4 (-6) -sided.</text>
      <biological_entity id="from_key_V2_301_o6823" name="branchlet" name_original="branchlets" src="d20_s3" type="structure" />
    </statement>
    <statement id="from_key_V2_301_d18_s0">
      <text>Abaxial glands conspicuous;</text>
    </statement>
    <statement id="from_key_V2_301_d18_s1">
      <text>dioecious (very rarely monoecious).</text>
      <biological_entity constraint="abaxial" id="from_key_V2_301_o6810" name="gland" name_original="glands" src="d18_s0" type="structure">
        <character is_modifier="false" name="prominence" src="d18_s0" value="conspicuous" />
        <character is_modifier="false" name="reproduction" src="d18_s1" value="dioecious" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_301_d16_s0">
      <text>Seed-cones variously colored, but with brownish or reddish hue, even when some blue present, with 1 (-2) seeds, usually maturing in 1 year;</text>
      <biological_entity id="from_key_V2_301_o6799" name="seed-cone" name_original="seed-cones" src="d16_s0" type="structure">
        <character is_modifier="false" modifier="variously" name="coloration" src="d16_s0" value="colored" />
        <character is_modifier="false" name="coloration" src="d16_s0" value="brownish" />
        <character is_modifier="false" name="coloration" src="d16_s0" value="reddish hue" />
        <character is_modifier="false" modifier="when some present" name="coloration" src="d16_s0" value="blue" />
        <character constraint="in year" constraintid="o6801" is_modifier="false" modifier="usually" name="life_cycle" notes="alterIDs:o6800" src="d16_s0" value="maturing" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6800" name="seed" name_original="seeds" src="d16_s0" type="structure">
        <character char_type="range_value" from="1" from_inclusive="false" is_modifier="true" name="atypical_count" src="d16_s0" to="2" />
        <character is_modifier="true" name="count" src="d16_s0" value="1" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6801" name="year" name_original="year" src="d16_s0" type="structure">
        <character is_modifier="true" name="count" src="d16_s0" value="1" />
      </biological_entity>
      <relation from="from_key_V2_301_o6799" id="from_key_V2_301_r1584" name="with" negation="false" src="d16_s0" to="from_key_V2_301_o6800" />
    </statement>
    <statement id="from_key_V2_301_d16_s1">
      <text>bark of branchlets greater than 10 mm diam. usually exfoliating in strips or smooth;</text>
      <biological_entity id="from_key_V2_301_o6802" name="bark" name_original="bark" src="d16_s1" type="structure">
        <character char_type="range_value" from="10" from_unit="mm" name="diameter" src="d16_s1" upper_restricted="false" />
        <character constraint="in strips" constraintid="o6804" is_modifier="false" modifier="usually" name="relief" src="d16_s1" value="exfoliating" />
        <character is_modifier="false" name="architecture_or_pubescence_or_relief" src="d16_s1" value="smooth" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6803" name="branchlet" name_original="branchlets" src="d16_s1" type="structure" />
      <biological_entity id="from_key_V2_301_o6804" name="strip" name_original="strips" src="d16_s1" type="structure" />
      <relation from="from_key_V2_301_o6802" id="from_key_V2_301_r1585" name="part_of" negation="false" src="d16_s1" to="from_key_V2_301_o6803" />
    </statement>
    <statement id="from_key_V2_301_d16_s2">
      <text>usually multistemmed shrubs or trees to 12 m.</text>
      <biological_entity id="from_key_V2_301_o6805" name="shrub" name_original="shrubs" src="d16_s2" type="structure">
        <character is_modifier="true" modifier="usually" name="architecture_or_growth_form" src="d16_s2" value="multistemmed" />
        <character char_type="range_value" from="0" from_unit="m" name="some_measurement" src="d16_s2" to="12" to_unit="m" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6806" name="tree" name_original="trees" src="d16_s2" type="structure">
        <character is_modifier="true" modifier="usually" name="architecture_or_growth_form" src="d16_s2" value="multistemmed" />
        <character char_type="range_value" from="0" from_unit="m" name="some_measurement" src="d16_s2" to="12" to_unit="m" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_301_d14_s0">
      <text>Leaves with ovate, elliptic, elongate, or inconspicuous glands (or glands round on scalelike leaves only).</text>
      <biological_entity id="from_key_V2_301_o6789" name="leaf" name_original="leaves" src="d14_s0" type="structure" />
      <biological_entity id="from_key_V2_301_o6790" name="gland" name_original="glands" src="d14_s0" type="structure">
        <character is_modifier="true" name="shape" src="d14_s0" value="ovate" />
        <character is_modifier="true" name="arrangement_or_shape" src="d14_s0" value="elliptic" />
        <character is_modifier="true" name="shape" src="d14_s0" value="elongate" />
        <character is_modifier="true" name="prominence" src="d14_s0" value="inconspicuous" />
      </biological_entity>
      <relation from="from_key_V2_301_o6789" id="from_key_V2_301_r1581" name="with" negation="false" src="d14_s0" to="from_key_V2_301_o6790" />
    </statement>
    <statement id="from_key_V2_301_d12_s0">
      <text>Seed-cones with 1-3 seeds, fleshy or resinous to fibrous;</text>
      <biological_entity id="from_key_V2_301_o6782" name="seed-cone" name_original="seed-cones" src="d12_s0" type="structure">
        <character is_modifier="false" name="texture" notes="alterIDs:o6783" src="d12_s0" value="fleshy" />
        <character is_modifier="false" name="coating" src="d12_s0" value="resinous" />
        <character is_modifier="false" name="texture" src="d12_s0" value="fibrous" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6783" name="seed" name_original="seeds" src="d12_s0" type="structure">
        <character char_type="range_value" from="1" is_modifier="true" name="count" src="d12_s0" to="3" />
      </biological_entity>
      <relation from="from_key_V2_301_o6782" id="from_key_V2_301_r1579" name="with" negation="false" src="d12_s0" to="from_key_V2_301_o6783" />
    </statement>
    <statement id="from_key_V2_301_d12_s1">
      <text>bark exfoliating in thin strips, branchlets erect.</text>
      <biological_entity id="from_key_V2_301_o6784" name="bark" name_original="bark" src="d12_s1" type="structure">
        <character constraint="in strips" constraintid="o6785" is_modifier="false" name="relief" src="d12_s1" value="exfoliating" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6785" name="strip" name_original="strips" src="d12_s1" type="structure">
        <character is_modifier="true" name="width" src="d12_s1" value="thin" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6786" name="branchlet" name_original="branchlets" src="d12_s1" type="structure">
        <character is_modifier="false" name="orientation" src="d12_s1" value="erect" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_301_d4_s0">
      <text>Margins of leaves denticulate (at 20Ã—), scalelike leaves usually obtuse to acute at apex (sometimes mucronate in Juniperus deppeana or acuminate in J. monosperma).</text>
      <biological_entity id="from_key_V2_301_o6733" name="margin" name_original="margins" src="d4_s0" type="structure">
        <character is_modifier="false" name="shape" src="d4_s0" value="denticulate" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6734" name="leaf" name_original="leaves" src="d4_s0" type="structure" />
      <biological_entity id="from_key_V2_301_o6735" name="leaf" name_original="leaves" src="d4_s0" type="structure">
        <character is_modifier="true" name="shape" src="d4_s0" value="scalelike" />
        <character is_modifier="false" modifier="usually" name="shape" notes="[duplicate value]" src="d4_s0" value="obtuse" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d4_s0" value="acute" />
        <character char_type="range_value" constraint="at apex" constraintid="o6736" from="usually obtuse" name="shape" src="d4_s0" to="acute" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6736" name="apex" name_original="apex" src="d4_s0" type="structure" />
      <relation from="from_key_V2_301_o6733" id="from_key_V2_301_r1569" name="part_of" negation="false" src="d4_s0" to="from_key_V2_301_o6734" />
    </statement>
    <statement id="from_key_V2_301_d2_s0">
      <text>Leaves of 2 kinds, whip (subulate, without basal abscission zone) and scalelike;</text>
      <biological_entity id="from_key_V2_301_o6725" name="leaf" name_original="leaves" src="d2_s0" type="structure">
        <character is_modifier="false" name="shape" notes="alterIDs:o6726" src="d2_s0" value="scalelike" />
      </biological_entity>
      <biological_entity id="from_key_V2_301_o6726" name="kind" name_original="kinds" src="d2_s0" type="structure">
        <character is_modifier="true" name="count" src="d2_s0" value="2" />
      </biological_entity>
      <relation from="from_key_V2_301_o6725" id="from_key_V2_301_r1566" name="consist_of" negation="false" src="d2_s0" to="from_key_V2_301_o6726" />
    </statement>
    <statement id="from_key_V2_301_d2_s1">
      <text>cones terminal (Juniperus sect. Sabina Spach).</text>
      <biological_entity id="from_key_V2_301_o6727" name="cone" name_original="cones" src="d2_s1" type="structure">
        <character is_modifier="false" name="position_or_structure_subtype" src="d2_s1" value="terminal" />
      </biological_entity>
    </statement>
  </description>
