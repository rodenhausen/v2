    <statement id="from_key_V2_611_d48_s0">
      <text>Trophophore blades ovate to oblong (nearly deltate in Botrychium hesperium, deltate-oblong in B. pinnatum);</text>
      <biological_entity constraint="trophophore" id="from_key_V2_611_o16896" name="blade" name_original="blades" src="d48_s0" type="structure">
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d48_s0" value="ovate" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d48_s0" value="oblong" />
        <character char_type="range_value" from="ovate" name="shape" src="d48_s0" to="oblong" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d48_s1">
      <text>sporophores with single midrib or 1 dominant midrib and 2 smaller ribs.</text>
      <biological_entity id="from_key_V2_611_o16897" name="sporophore" name_original="sporophores" src="d48_s1" type="structure" />
      <biological_entity id="from_key_V2_611_o16898" name="midrib" name_original="midrib" src="d48_s1" type="structure">
        <character is_modifier="true" name="count" src="d48_s1" value="single" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16899" name="midrib" name_original="midrib" src="d48_s1" type="structure">
        <character is_modifier="true" name="count" src="d48_s1" value="1" />
        <character is_modifier="true" name="prominence" src="d48_s1" value="dominant" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16900" name="rib" name_original="ribs" src="d48_s1" type="structure">
        <character is_modifier="true" name="count" src="d48_s1" value="1" />
        <character is_modifier="true" name="prominence" src="d48_s1" value="dominant" />
      </biological_entity>
      <biological_entity constraint="smaller" id="from_key_V2_611_o16901" name="midrib" name_original="midrib" src="d48_s1" type="structure" />
      <relation from="from_key_V2_611_o16897" id="from_key_V2_611_r3941" name="with" negation="false" src="d48_s1" to="from_key_V2_611_o16898" />
      <relation from="from_key_V2_611_o16897" id="from_key_V2_611_r3942" name="with" negation="false" src="d48_s1" to="from_key_V2_611_o16899" />
      <relation from="from_key_V2_611_o16897" id="from_key_V2_611_r3943" name="with" negation="false" src="d48_s1" to="from_key_V2_611_o16900" />
    </statement>
    <statement id="from_key_V2_611_d46_s0">
      <text>Trophophore present, fully distinct from sporophore.</text>
      <biological_entity id="from_key_V2_611_o16891" name="trophophore" name_original="trophophore" src="d46_s0" type="structure">
        <character is_modifier="false" name="count" src="d46_s0" value="present" />
        <character constraint="from sporophore" constraintid="o16892" is_modifier="false" modifier="fully" name="fusion" src="d46_s0" value="distinct" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16892" name="sporophore" name_original="sporophore" src="d46_s0" type="structure" />
    </statement>
    <statement id="from_key_V2_611_d28_s0">
      <text>Trophophores present or replaced by sporophore;</text>
    </statement>
    <statement id="from_key_V2_611_d28_s1">
      <text>if present, basal pinnae or segment venation pinnate, midrib present, oblanceolate to linear to lanceolate to ovate.</text>
      <biological_entity id="from_key_V2_611_o16827" name="trophophore" name_original="trophophores" src="d28_s0" type="structure">
        <character is_modifier="false" name="count" src="d28_s0" value="present" />
        <character name="count" src="d28_s0" value="replaced by sporophore" />
        <character is_modifier="false" name="count" src="d28_s1" value="present" />
      </biological_entity>
      <biological_entity constraint="basal" id="from_key_V2_611_o16828" name="pinna" name_original="pinnae" src="d28_s1" type="structure">
        <character is_modifier="false" name="architecture_or_shape" src="d28_s1" value="pinnate" />
      </biological_entity>
      <biological_entity constraint="basal" id="from_key_V2_611_o16829" name="segment" name_original="segment" src="d28_s1" type="structure">
        <character is_modifier="false" name="architecture_or_shape" src="d28_s1" value="pinnate" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16830" name="midrib" name_original="midrib" src="d28_s1" type="structure">
        <character is_modifier="false" name="count" src="d28_s1" value="present" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d28_s1" value="oblanceolate" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d28_s1" value="linear" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d28_s1" value="lanceolate" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d28_s1" value="ovate" />
        <character char_type="range_value" from="oblanceolate" name="shape" src="d28_s1" to="linear" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d24_s0">
      <text>Distance between 1st and 2d pinna pairs same or slightly more than between 2d and 3d pairs;</text>
      <biological_entity constraint="between pinna" constraintid="o16804" id="from_key_V2_611_o16803" name="whole_organism" name_original="" src="d24_s0" type="structure" />
      <biological_entity id="from_key_V2_611_o16804" name="pinna" name_original="pinna" src="d24_s0" type="structure" />
    </statement>
    <statement id="from_key_V2_611_d24_s1">
      <text>segments asymmetric to symmetric.</text>
      <biological_entity id="from_key_V2_611_o16805" name="segment" name_original="segments" src="d24_s1" type="structure">
        <character is_modifier="false" name="architecture_or_shape" notes="[duplicate value]" src="d24_s1" value="asymmetric" />
        <character is_modifier="false" name="architecture_or_shape" notes="[duplicate value]" src="d24_s1" value="symmetric" />
        <character char_type="range_value" from="asymmetric" name="architecture_or_shape" src="d24_s1" to="symmetric" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d18_s0">
      <text>Trophophores linear to deltate (narrowly oblong in Botrychium minganese), pinnate, rarely simple, lobes, if present, of various shapes, stalk usually less than 1/4 length of trophophore;</text>
      <biological_entity id="from_key_V2_611_o16780" name="trophophore" name_original="trophophores" src="d18_s0" type="structure">
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d18_s0" value="linear" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d18_s0" value="deltate" />
        <character char_type="range_value" from="linear" name="shape" src="d18_s0" to="deltate" />
        <character is_modifier="false" name="architecture_or_shape" src="d18_s0" value="pinnate" />
        <character is_modifier="false" modifier="rarely" name="architecture" src="d18_s0" value="simple" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16781" name="lobe" name_original="lobes" src="d18_s0" type="structure">
        <character is_modifier="false" name="count" src="d18_s0" value="present" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16782" name="stalk" name_original="stalk" src="d18_s0" type="structure">
        <character char_type="range_value" from="0 length of trophophore" modifier="of various shapes" name="length" src="d18_s0" to="1/4 length of trophophore" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d18_s1">
      <text>plants usually in exposed sites.</text>
      <biological_entity id="from_key_V2_611_o16783" name="plant" name_original="plants" src="d18_s1" type="structure" />
      <biological_entity id="from_key_V2_611_o16784" name="site" name_original="sites" src="d18_s1" type="structure">
        <character is_modifier="true" name="prominence" src="d18_s1" value="exposed" />
      </biological_entity>
      <relation from="from_key_V2_611_o16783" id="from_key_V2_611_r3930" name="in" negation="false" src="d18_s1" to="from_key_V2_611_o16784" />
    </statement>
    <statement id="from_key_V2_611_d2_s0">
      <text>Leaf-blades mainly oblong to linear, mostly 2-4 cm, all fertile, sporophores always present;</text>
      <biological_entity id="from_key_V2_611_o16700" name="leaf-blade" name_original="leaf-blades" src="d2_s0" type="structure">
        <character is_modifier="false" modifier="mainly" name="shape" notes="[duplicate value]" src="d2_s0" value="oblong" />
        <character is_modifier="false" name="shape" notes="[duplicate value]" src="d2_s0" value="linear" />
        <character char_type="range_value" from="mainly oblong" name="shape" src="d2_s0" to="linear" />
        <character char_type="range_value" from="2" from_unit="cm" modifier="mostly" name="distance" src="d2_s0" to="4" to_unit="cm" />
        <character is_modifier="false" name="reproduction" src="d2_s0" value="fertile" />
      </biological_entity>
      <biological_entity id="from_key_V2_611_o16701" name="sporophore" name_original="sporophores" src="d2_s0" type="structure">
        <character is_modifier="false" modifier="always" name="count" src="d2_s0" value="present" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d2_s1">
      <text>plants to 15 cm, mostly less than 10 cm;</text>
      <biological_entity id="from_key_V2_611_o16702" name="plant" name_original="plants" src="d2_s1" type="structure">
        <character char_type="range_value" from="0" from_unit="cm" name="some_measurement" src="d2_s1" to="15" to_unit="cm" />
        <character char_type="range_value" from="0" from_unit="cm" modifier="mostly" name="some_measurement" src="d2_s1" to="10" to_unit="cm" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_611_d2_s2">
      <text>leaf-sheaths closed (subg. Botrychium).</text>
      <biological_entity id="from_key_V2_611_o16703" name="leaf-sheath" name_original="leaf-sheaths" src="d2_s2" type="structure">
        <character is_modifier="false" name="condition" src="d2_s2" value="closed" />
      </biological_entity>
    </statement>
  </description>
