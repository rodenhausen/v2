      <biological_entity id="from_key_V2_451_o11004" name="primary-vein" name_original="primary-veins" src="d2_s1" type="structure" />
      <biological_entity id="from_key_V2_451_o11005" name="vein" name_original="vein" src="d2_s1" type="structure">
        <character is_modifier="true" name="count" src="d2_s1" value="common" />
