      <text>blades adaxially lacking glands;</text>
      <biological_entity id="from_key_V2_451_o11157" name="blade" name_original="blades" src="d34_s1" type="structure" />
      <biological_entity id="from_key_V2_451_o11158" name="gland" name_original="glands" src="d34_s1" type="structure">
        <character is_modifier="true" modifier="adaxially" name="count" src="d34_s1" value="lacking" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d34_s2">
      <text>a few scales often persistent on rachises and/or costae abaxially.</text>
      <biological_entity id="from_key_V2_451_o11159" name="scale" name_original="scales" src="d34_s2" type="structure">
        <character is_modifier="true" name="count" src="d34_s2" value="few" />
        <character constraint="on rachises, costae" constraintid="o11160, o11161" is_modifier="false" modifier="often" name="duration" src="d34_s2" value="persistent" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11160" name="rachis" name_original="rachises" src="d34_s2" type="structure" />
      <biological_entity id="from_key_V2_451_o11161" name="costa" name_original="costae" src="d34_s2" type="structure" />
    </statement>
    <statement id="from_key_V2_451_d32_s0">
      <text>Stems long to short-creeping;</text>
      <biological_entity id="from_key_V2_451_o11142" name="stem" name_original="stems" src="d32_s0" type="structure">
        <character is_modifier="false" name="length_or_size" src="d32_s0" value="long" />
        <character is_modifier="false" name="growth_form_or_orientation" src="d32_s0" value="short-creeping" />
      </biological_entity>
    </statement>
    <statement id="from_key_V2_451_d32_s1">
      <text>scales at base of petioles lanceolate, usually hairy.</text>
      <biological_entity id="from_key_V2_451_o11143" name="scale" name_original="scales" src="d32_s1" type="structure">
        <character is_modifier="false" modifier="usually" name="pubescence" notes="alterIDs:o11144" src="d32_s1" value="hairy" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11144" name="base" name_original="base" src="d32_s1" type="structure">
        <character is_modifier="false" name="shape" src="d32_s1" value="lanceolate" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11145" name="petiole" name_original="petioles" src="d32_s1" type="structure" />
      <relation from="from_key_V2_451_o11143" id="from_key_V2_451_r2550" name="at" negation="false" src="d32_s1" to="from_key_V2_451_o11144" />
      <relation from="from_key_V2_451_o11144" id="from_key_V2_451_r2551" name="part_of" negation="false" src="d32_s1" to="from_key_V2_451_o11145" />
    </statement>
    <statement id="from_key_V2_451_d26_s0">
      <text>Basal veins of adjacent segments free, or connivent at sinuses.</text>
      <biological_entity constraint="basal" id="from_key_V2_451_o11104" name="vein" name_original="veins" src="d26_s0" type="structure">
        <character is_modifier="false" name="fusion" src="d26_s0" value="free" />
        <character constraint="at sinuses" constraintid="o11106" is_modifier="false" name="arrangement" src="d26_s0" value="connivent" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11105" name="segment" name_original="segments" src="d26_s0" type="structure">
        <character is_modifier="true" name="arrangement" src="d26_s0" value="adjacent" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11106" name="sinuse" name_original="sinuses" src="d26_s0" type="structure" />
      <relation from="from_key_V2_451_o11104" id="from_key_V2_451_r2539" name="part_of" negation="false" src="d26_s0" to="from_key_V2_451_o11105" />
    </statement>
    <statement id="from_key_V2_451_d14_s0">
      <text>Blades with at least some basal veins extending to, connivent at, or united below sinus;</text>
      <biological_entity id="from_key_V2_451_o11054" name="blade" name_original="blades" src="d14_s0" type="structure" />
      <biological_entity constraint="basal" id="from_key_V2_451_o11055" name="vein" name_original="veins" src="d14_s0" type="structure" />
      <biological_entity id="from_key_V2_451_o11056" name="sinus" name_original="sinus" src="d14_s0" type="structure">
        <character is_modifier="true" name="arrangement" src="d14_s0" value="connivent" />
        <character is_modifier="true" name="fusion" src="d14_s0" value="united" />
      </biological_entity>
      <relation from="from_key_V2_451_o11054" id="from_key_V2_451_r2526" name="with" negation="false" src="d14_s0" to="from_key_V2_451_o11055" />
      <relation from="from_key_V2_451_o11055" id="from_key_V2_451_r2527" name="extending to" negation="false" src="d14_s0" to="from_key_V2_451_o11056" />
    </statement>
    <statement id="from_key_V2_451_d14_s1">
      <text>proximal pinnae usually not reduced (subg. Cyclosorus), or if reduced, then veins united below sinus.</text>
      <biological_entity constraint="proximal" id="from_key_V2_451_o11057" name="pinna" name_original="pinnae" src="d14_s1" type="structure">
        <character is_modifier="false" modifier="usually not" name="size" src="d14_s1" value="reduced" />
        <character is_modifier="false" name="size" src="d14_s1" value="reduced" />
      </biological_entity>
      <biological_entity id="from_key_V2_451_o11058" name="vein" name_original="veins" src="d14_s1" type="structure" />
      <biological_entity id="from_key_V2_451_o11059" name="sinus" name_original="sinus" src="d14_s1" type="structure" />
      <relation from="from_key_V2_451_o11058" id="from_key_V2_451_r2528" name="united" negation="false" src="d14_s1" to="from_key_V2_451_o11059" />
    </statement>
    <statement id="from_key_V2_451_d12_s0">
      <text>Sori round or slightly oblong;</text>
      <biological_entity id="from_key_V2_451_o11047" name="sorus" name_original="sori" src="d12_s0" type="structure">
        <character is_modifier="false" name="shape" src="d12_s0" value="round" />
