      <biological_entity constraint="adaxial" id="from_key_V2_17_o2483" name="surface" name_original="surface" src="d12_s2" type="structure" />
    </statement>
    <statement id="from_key_V2_17_d9_s0">
      <text>Indusia composed of relatively broad segments, these multiseriate for most of length but often branched or divided distally.</text>
      <biological_entity id="from_key_V2_17_o2469" name="indusium" name_original="indusia" src="d9_s0" type="structure">
        <character constraint="for" is_modifier="false" name="length" notes="alterIDs:o2470" src="d9_s0" value="multiseriate" />
