        <character is_modifier="false" modifier="sparsely to moderately" name="architecture_or_function_or_pubescence" src="d12_s1" value="glandular" />
        <character is_modifier="false" modifier="rarely" name="coating" src="d12_s1" value="viscid" />
      </biological_entity>
