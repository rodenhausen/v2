      <relation from="from_key_V2_300_o6299" id="from_key_V2_300_r1447" name="part_of" negation="false" src="d1_s1" to="from_key_V2_300_o6300" />
      <relation from="from_key_V2_300_o6299" id="from_key_V2_300_r1448" name="shed early" negation="false" src="d1_s1" to="from_key_V2_300_o6301" />
      <relation from="from_key_V2_300_o6302" id="from_key_V2_300_r1449" name="with" negation="false" src="d1_s1" to="from_key_V2_300_o6303" />
    </statement>
    <statement id="from_key_V2_300_d1_s2">
      <text>seed-cones unarmed (except in P. balfouriana, P. aristata, P. longaeva) (subg. Strobus).</text>
      <biological_entity id="from_key_V2_300_o6304" name="seed-cone" name_original="seed-cones" src="d1_s2" type="structure">
        <character is_modifier="false" name="architecture" src="d1_s2" value="unarmed" />
