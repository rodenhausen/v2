        <character is_modifier="false" name="length_or_size" src="d4_s1" value="slightly longer than wide" />
      </biological_entity>
      <biological_entity id="from_key_V2_17_o2438" name="base" name_original="base" src="d4_s1" type="structure" />
      <biological_entity id="from_key_V2_17_o2439" name="cell" name_original="cells" src="d4_s1" type="structure" />
