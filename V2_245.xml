      </biological_entity>
      <biological_entity id="from_key_V2_17_o2466" name="midrib" name_original="midrib" src="d8_s0" type="structure" />
      <relation from="from_key_V2_17_o2465" id="from_key_V2_17_r573" name="along" negation="false" src="d8_s0" to="from_key_V2_17_o2466" />
    </statement>
    <statement id="from_key_V2_17_d8_s1">
      <text>mature petioles light-brown to straw-colored or, if reddish-brown/dark purple (in Woodsia plummerae and W. oregana), somewhat pliable and resistant to shattering.</text>
      <biological_entity id="from_key_V2_17_o2467" name="petiole" name_original="petioles" src="d8_s1" type="structure">
        <character is_modifier="true" name="life_cycle" src="d8_s1" value="mature" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" src="d8_s1" value="light-brown" />
        <character is_modifier="false" name="coloration" notes="[duplicate value]" src="d8_s1" value="straw-colored" />
        <character char_type="range_value" from="light-brown" name="coloration" src="d8_s1" to="straw-colored" />
        <character name="coloration" src="d8_s1" value="," />
        <character is_modifier="false" name="coloration" src="d8_s1" value="reddish-brown/dark purple" />
        <character is_modifier="false" modifier="somewhat" name="fragility_or_texture" src="d8_s1" value="pliable" />
      </biological_entity>
      <biological_entity id="from_key_V2_17_o2468" name="shattering" name_original="shattering" src="d8_s1" type="structure" />
      <relation from="from_key_V2_17_o2467" id="from_key_V2_17_r574" name="to" negation="false" src="d8_s1" to="from_key_V2_17_o2468" />
    </statement>
    <statement id="from_key_V2_17_d4_s0">
      <text>Petioles not articulate above base, swollen abscission zone absent;</text>
      <biological_entity id="from_key_V2_17_o2434" name="petiole" name_original="petioles" src="d4_s0" type="structure">
        <character constraint="above base" constraintid="o2435" is_modifier="false" modifier="not" name="architecture" src="d4_s0" value="articulate" />
      </biological_entity>
      <biological_entity id="from_key_V2_17_o2435" name="base" name_original="base" src="d4_s0" type="structure" />
      <biological_entity id="from_key_V2_17_o2436" name="zone" name_original="zone" src="d4_s0" type="structure">
