    </statement>
    <statement id="from_key_V2_17_d16_s1">
      <text>proximal portion of mature petioles usually light-brown or straw-colored (sometimes darker at very base);</text>
      <biological_entity constraint="proximal" id="from_key_V2_17_o2511" name="portion" name_original="portion" src="d16_s1" type="structure">
        <character is_modifier="false" modifier="usually" name="coloration" src="d16_s1" value="light-brown" />
        <character is_modifier="false" name="coloration" src="d16_s1" value="straw-colored" />
      </biological_entity>
      <biological_entity id="from_key_V2_17_o2512" name="petiole" name_original="petioles" src="d16_s1" type="structure">
        <character is_modifier="true" name="life_cycle" src="d16_s1" value="mature" />
      </biological_entity>
      <relation from="from_key_V2_17_o2511" id="from_key_V2_17_r589" name="part_of" negation="false" src="d16_s1" to="from_key_V2_17_o2512" />
    </statement>
    <statement id="from_key_V2_17_d16_s2">
      <text>indusial filaments generally apparent, often greatly surpassing mature sporangia.</text>
      <biological_entity constraint="indusial" id="from_key_V2_17_o2513" name="filament" name_original="filaments" src="d16_s2" type="structure">
        <character is_modifier="false" modifier="generally" name="prominence" src="d16_s2" value="apparent" />
      </biological_entity>
      <biological_entity id="from_key_V2_17_o2514" name="sporangium" name_original="sporangia" src="d16_s2" type="structure">
        <character is_modifier="true" name="life_cycle" src="d16_s2" value="mature" />
      </biological_entity>
      <relation from="from_key_V2_17_o2513" id="from_key_V2_17_r590" modifier="often greatly" name="surpassing" negation="false" src="d16_s2" to="from_key_V2_17_o2514" />
    </statement>
    <statement id="from_key_V2_17_d10_s0">
      <text>Indusia composed of narrow, usually filamentous segments, these uniseriate for most of length.</text>
      <biological_entity id="from_key_V2_17_o2471" name="indusium" name_original="indusia" src="d10_s0" type="structure" />
      <biological_entity id="from_key_V2_17_o2472" name="segment" name_original="segments" src="d10_s0" type="structure">
        <character is_modifier="true" name="architecture_or_arrangement" src="d10_s0" value="uniseriate" />
        <character is_modifier="true" name="length" src="d10_s0" value="narrow" />
        <character is_modifier="true" modifier="usually" name="texture" src="d10_s0" value="filamentous" />
      </biological_entity>
      <relation from="from_key_V2_17_o2471" id="from_key_V2_17_r576" name="composed of" negation="false" src="d10_s0" to="from_key_V2_17_o2472" />
    </statement>
    <statement id="from_key_V2_17_d8_s0">
      <text>Pinnae lacking flattened, multicellular hairs along midrib;</text>
      <biological_entity id="from_key_V2_17_o2464" name="pinna" name_original="pinnae" src="d8_s0" type="structure">
        <character is_modifier="false" name="count" src="d8_s0" value="lacking" />
